          "annee": 1991,
          "duree": 99,
          "version": "",
          "format": "",
          "ageMinimal": null,
          "generique": [
            "Anjelica Huston",
            "Christopher Lloyd",
            "Raúl Juliá",
            "Christina Ricci"
          ],
          "textes": [
            {
              "typeTexte": 203,
              "texte": "Quelle étrange famille, ces Addams ! On les croirait tout droit sortis d'un film d'horreur, mais quand on apprend à les connaître, ils ont tous un très grand cœur. Il ne faut pas se fier aux apparences !"
            }
