          "annee": 2011,
          "pays": "France",
          "duree": 12,
          "version": "",
          "format": "DCP",
          "ageMinimal": null,
          "generique": [
            "Finnegan Oldfield",
            "Garance Marillier"
          ],
          "textes": [
            {
              "typeTexte": 9,
              "texte": "_Le Secret de Brokeback Mountain_ est passé hier soir à la télé. Boulerversé par cette découverte, Vincent raconte le film à son copain de lycée Moussa."
            }
          ]
        },
        {
          "idSeance": 33029,
          "ordre": 1,
          "dateHeure": "2020-02-10T19:00:00",
