          "idFilm": 143178,
          "titre": "Au cœur du monde",
          "titreVo": "No coração do mundo",
          "realisateurs": "Gabriel Martins, Maurilio Martins",
          "annee": 2019,
          "pays": "Brésil",
          "duree": 120,
          "version": "VOSTF",
          "format": "DCP",
