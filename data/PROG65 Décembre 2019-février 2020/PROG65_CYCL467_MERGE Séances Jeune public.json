          "typeEvenement": 13,
          "idFilm": 128558,
          "titre": "Avril et le monde truqué",
          "titreVo": "",
          "realisateurs": "Franck Ekinci, Christian Desmares",
          "annee": 2014,
          "pays": "France-Canada-Belgique",
          "duree": 105,
          "version": "VF",
          "format": "DCP",
          "ageMinimal": 8,
          "generique": [
            "Marion Cotillard",
            "Philippe Katerine",
            "Jean Rochefort"
          ],
          "textes": [
            {
              "typeTexte": 203,
              "texte": "Dans ce monde étrange où deux tours Eiffel dominent Paris, Avril devra faire preuve de ruse et de courage pour arriver retrouver ses parents deux scientifiques qui ont mystérieusement disparu."
            },
            {
              "typeTexte": 203,
