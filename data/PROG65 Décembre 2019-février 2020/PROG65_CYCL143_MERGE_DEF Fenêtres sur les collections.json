          "titre": "Fabrication du charbon de bois en Bretagne",
          "titreVo": "",
          "realisateurs": "anonyme",
          "annee": 1911,
          "pays": "France",
          "duree": 5,
          "version": "INT. FR.",
          "format": "DCP",
          "ageMinimal": null,
          "textes": [
            {
              "typeTexte": 9,
              "texte": "Procédé artisanal de la fabrication du charbon : de la construction de la meule à la mise en sac."
            }
          ]
        },
        {
          "idSeance": 32943,
          "ordre": 3,
          "dateHeure": "2019-12-06T18:30:00",
          "idSalle": [
