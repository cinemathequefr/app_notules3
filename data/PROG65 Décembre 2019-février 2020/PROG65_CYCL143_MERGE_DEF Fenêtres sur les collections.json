          "typeEvenement": 13,
          "idFilm": 142937,
          "titre": "Vichy : Source de beauté : Bikini",
          "titreVo": "",
          "realisateurs": "anonyme",
          "annee": 1946,
          "pays": "France",
          "duree": 2,
          "version": "",
          "format": "DCP",
          "ageMinimal": null,
          "textes": [
            {
              "typeTexte": 9,
              "texte": "Quand l'industrie cosmétique et l'industrie nucléaire se rencontrent le temps d'un spot publicitaire."
            }
          ]
        },
        {
          "idSeance": 32944,
          "ordre": 2,
